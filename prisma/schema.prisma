datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

//Criando a tabela users no banco de dados 
model User{
  id     String @id @default(uuid())
  name String
  github_id Int
  avatar_url String
  login String

  @@map("users")
}